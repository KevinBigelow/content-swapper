{
  "manifest_version": 2,

  "name": "SwappyCopy",
  "version": "0.1",
  "description": "Manipulate the DOM when the page is done loading",

  "content_scripts": [ {
    "js": [ "jquery-3.2.1.min.js", "content.js" ],
    "matches": [ "http://*/*", "https://*/*"],
    "run_at": "document_end"
  }],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
